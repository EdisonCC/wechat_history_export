{
    "manifest_version": 2,
    "name": "WechatExporter",
    "version": "1.0",
    "description": "把某个公众号的历史文章全部导出为 Excel 表格或 PDF 文档。",
    "icons": {
        "16": "images/Sticker_Icon_Rainbow.png",
        "48": "images/Sticker_Icon_Rainbow.png",
        "128": "images/Sticker_Icon_Rainbow.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "images/Sticker_Icon_Sun.png",
            "38": "images/Sticker_Icon_Sun.png"
        },
        "default_title": "微信公众号文章导出",
        "default_popup": "popup.html"
    }
}